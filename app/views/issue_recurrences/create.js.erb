<% if @recurrence.errors.present? %>
  $('#recurrence-errors').html(
    '<%= escape_javascript(nameless_error_messages_for 'recurrence') %>'
  );
<% else %>
  $('#new-recurrence').empty();
  $('#recurrences')
    .html('<%= escape_javascript(render :partial => 'issues/recurrences/index') %>');
<% end %>
